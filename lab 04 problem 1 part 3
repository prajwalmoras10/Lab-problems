# cumulative total of numbers in a sequence
my_list = []

count = int(input("How many numbers you want to add : "))

for i in range(1, count+1):
    my_list.append(int(input("Enter number {} : ".format(i))))

print("Input Numbers : ")
print(my_list)

j = [sum(my_list[0:count+1])for count in range(0, len(my_list))]

print("original list is :", my_list)
print()
print("cumulative sum is:", j)
